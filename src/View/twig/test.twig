                <script type="text/javascript">
                    jQuery(document).ready(function() {

                        // for toggling between "basic" and "advanced" terminology
                        function switchSearchTerm() {

                            if ($(".advSearch").hasClass('basic')) {
                                $(".advSearch").html('<span class="toggle-icon"></span>More Options');
                                $(".advSearch").removeClass('basic');
                                $(".options").removeClass('active');
                            } else {
                                $(".advSearch").html('<span class="toggle-icon"></span>Fewer Options');
                                $(".advSearch").addClass('basic');
                                $(".options").addClass('active');
                            }//closes if-else

                        }//closes function

                        // establish if advanced search format or other limiter has been selected/set with PHP
                        var advFormat = "";
                        var advOther = new Array("default");
                        var currentUser = "";

                        // when not used, hide advanced search options (also applies to "new" browse)
                        if (advFormat == "" && (advOther == "" || advOther[0] == "new" || advOther[0] == "popular" || advOther[0] == "default")) {
                            $('.advanced-options').hide();
                        } else {
                            switchSearchTerm();
                        }//closes if-else

                        // toggle basic and advanced options when advanced search link clicked
                        $(".advSearch").click(function() {
                            $('.advanced-options').toggle();
                            switchSearchTerm();
                        });//closes click

                        $('#search').keypress(function (e) {
                            if (e.which == 13) {
                                $('form#resources-search').submit();
                                return false;
                            }
                        });

                        // function to change the visible items, given title, keyword, subject, format and any other limiters
                        function loadAJAX(title, search, subject, format, other) {

                            jQuery.ajax({

                                type: 'get',
                                dataType: 'jsonp',
                                url: 'https://resources.library.ubc.ca/includes/resource_list.inc.php', //php file that handles the submission
                                data: {
                                    title: title,
                                    search: search,
                                    subject: subject,
                                    format: format,
                                    other: other,
                                    current_user: '' }, //the "get" info passed along
                                async: false,
                                success: function(data) { //what to do on successful return (replace the resources-list div, scroll to results if applicable, call colorbox)
                                    if (data && data.html) {

                                        var fixedstring;
                                        try {
                                            fixedstring = decodeURIComponent(escape(data.html));
                                        } catch(e) {
                                            fixedstring = data.html;
                                        }

                                        jQuery("#resources-list").html(fixedstring);
                                        if ($("#search-results").length > 0) {
                                            jQuery('html, body').animate({
                                                scrollTop: $("#search-results").offset().top
                                            }, 800);
                                        } else {
                                            jQuery('html, body').animate({
                                                scrollTop: $("#search-box").offset().top
                                            }, 800);
                                        }//closes if-else
                                    } else {
                                        jQuery("#resources-list").html("<h3>Problem encountered with results</h3><p>An error occurred with this search or browse request. <strong>Please try again from the <a href=\"/\">Indexes and Databases main page</a>.</strong></p><p>If this error persists and prevents you from finding your resource, please <a href=\"http://about.library.ubc.ca/contact-us/ejournal-help/\">contact e-Resources</a> (using the linked-to form or at 604-822-4192). We apologize for the inconvenience.</p>");
                                    }
                                    jQuery(".colorbox a").colorbox({
                                        width:"90%",
                                        height:"90%",
                                        iframe:true,
                                        onOpen:function(){
                                            $("#colorbox").css('display','block');
                                        }
                                    });
                                }

                            });//closes ajax

                        }//closes function

                        // add ajax to "pagination" A-Z letters, title browse and trial resource browse
                        $(".href").click(function() {

                            // "a" tag title is the attribute to pass in (as "title" or "other")
                            var href = $(this).attr("title");

                            // accommodate trial and popular resource browse links
                            if (href == "new" || href == "popular") {
                                // want to pass this in array form to work with other limiters in one parameter
                                var hrefarray = new Array(href);
                                loadAJAX("", "", "", "", hrefarray);
                            } else if (href == "all" || href == "") {
                                loadAJAX("", "", "", "", "default");
                            } else if (href.length == 1) {
                                loadAJAX(href);
                            }//closes if-elseif-else

                        });//closes href.click

                        // add ajax to subjects in browse subjects select box
                        $("#subject li").click(function() {

                            // title value is the subject ID to pass in
                            var subjectID = $(this).attr("title");
                            loadAJAX("", "", subjectID);

                        });//closes subject.change

                        $('.subjectSearch').on("click", function(event) {
                            $("#loading").show();
                            event.preventDefault();
                            search = $(this).attr("id");
